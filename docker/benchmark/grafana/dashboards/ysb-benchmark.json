{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Throughput (events/sec)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(isotope_rows_processed_total[5s])",
          "legendFormat": "{{operator_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "End-to-End Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(isotope_batch_latency_seconds_bucket[5s]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(isotope_batch_latency_seconds_bucket[5s]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(isotope_batch_latency_seconds_bucket[5s]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "GC Pauses",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_sum[1m])",
          "legendFormat": "GC time/sec"
        },
        {
          "expr": "go_gc_duration_seconds{quantile=\"0.75\"}",
          "legendFormat": "p75 pause"
        },
        {
          "expr": "go_gc_duration_seconds{quantile=\"1\"}",
          "legendFormat": "max pause"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes",
          "legendFormat": "heap alloc"
        },
        {
          "expr": "go_memstats_sys_bytes",
          "legendFormat": "sys total"
        },
        {
          "expr": "go_memstats_heap_inuse_bytes",
          "legendFormat": "heap in-use"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "Batches Processed",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(isotope_batches_processed_total)",
          "legendFormat": "total batches"
        }
      ]
    },
    {
      "title": "Total Rows Processed",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(isotope_rows_processed_total)",
          "legendFormat": "total rows"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["isotope", "ysb", "benchmark"],
  "templating": { "list": [] },
  "time": { "from": "now-5m", "to": "now" },
  "title": "Isotope YSB Benchmark",
  "uid": "isotope-ysb"
}
